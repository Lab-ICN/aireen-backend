{
  "name": "aireen-backend",
  "module": "src/main.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev:app": "APP_MODE=app bun run src/main.ts",
    "dev:worker": "APP_MODE=worker bun run src/main.ts",
    "build": "bun build ./src/main.ts --outdir dist",
    "start:app": "APP_MODE=app bun run dist/main.js",
    "start:worker": "APP_MODE=worker bun run dist/main.js"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/express": "^5.0.5",
    "@types/mqtt": "^2.5.0"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@influxdata/influxdb-client": "^1.35.0",
    "express": "^5.1.0",
    "mqtt": "^5.14.1",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2"
  }
}
